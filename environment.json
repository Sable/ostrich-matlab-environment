{
    "type": "environment",
    "short-name": "matlab-vm-2014b-jit",
    "name": "MATLAB Virtual Machine",
    "version": "R2014b (8.4.0.150421) 64-bit (glnxa64)",
    "supported-languages": ["matlab"],
    "supported-formats": ["text"],
    "implementation-language": "x86_64",
    "implementation-format": "binary",
    "executable-path": "/usr/local/pkgs/matlab/R2014b/bin/matlab",
    "executable-options": [
        "-nodesktop", 
        "-nodisplay", 
        "-r"
    ],
    "run-script-options": {
        "matlab-jit": true
    }
}
