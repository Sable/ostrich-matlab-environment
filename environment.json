{
	"type": "environment",
	"short-name": "matlab-2016a-jit",
	"name": "MATLAB Virtual Machine",
	"version": "R2016a (9.0.0.341360) 64-bit (glnxa64)",
	"supported-languages": ["matlab"],
	"supported-formats": ["text"],
	"implementation-language": "x86",
	"implementation-format": "binary",
	"executable-path": "/usr/local/pkgs/matlab/R2016a/bin/matlab",
	"executable-options": [
		"-nodesktop", 
		"-nodisplay", 
		"-r"
	],
	"run-script-options": {
		"matlab-jit": true
	}
}
