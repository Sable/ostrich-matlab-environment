{
	"type": "environment",
	"short-name": "matlab-2015b-jit",
	"name": "MATLAB Virtual Machine",
	"version": "R2015b (8.6.0.267246)",
	"supported-languages": ["matlab"],
	"supported-formats": ["text"],
	"implementation-language": "x86",
	"implementation-format": "binary",
	"executable-path": "/usr/local/pkgs/matlab/R2015b/bin/matlab",
	"executable-options": [
		"-nodesktop", 
		"-nodisplay", 
		"-r"
	],
	"run-script-options": {
		"matlab-jit": true
	}
}
